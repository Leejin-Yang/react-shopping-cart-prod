{
  "singleQuote": true,
  "printWidth": 80,
  "endOfLine": "auto",
  "arrowParens": "always",
  "bracketSameLine": false,
  "importOrder": [
    "^(@|pages)(.*|$)",
    "^(@|components)(.*|$)",
    "^(@|redux/)(.*|$)",
    "^(@|hooks)(.*|$)",
    "^(@|styles)(.*|$)",
    "^(@|(constants|api))(.*|$)"
  ],
  "importOrderSeparation": true
}
